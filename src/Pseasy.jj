String operacionAritmetica():{String e1=""; String e2="";}{
    e1 = nivelSegundoJerarquia() (<SUMA>  e2=nivelSegundoJerarquia() {e1=generarOperacionAritmetica("+",e1,e2);}
                        |<RESTA> e2=nivelSegundoJerarquia() {e1=generarOperacionAritmetica("-",e1,e2);})*
                                {return e1;}
}
//Funciones para la jerarquia de operadores
String nivelSegundoJerarquia():{String e1; String e2;}{
    e1 = terminalesNumeros() (<MULTIPLICACION> e2=terminalesNumeros() {e1=generarOperacionAritmetica("*",e1,e2);}
                              |<DIVISION> e2=terminalesNumeros() {e1=generarOperacionAritmetica("/",e1,e2);}
                              )*
                              {return e1;}
}
